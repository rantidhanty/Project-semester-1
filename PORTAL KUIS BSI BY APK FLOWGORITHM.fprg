                                                    <input variable="jawaban"/>
                                                    <if expression="jawaban = &quot;B&quot;">
                                                        <then>
                                                            <assign variable="nilai" expression="40"/>
                                                            <output expression="&quot; NILAI BENAR = 2&#13;&#10;  NILAI SALAH = 3&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <assign variable="nilai" expression="20"/>
                                                            <output expression="&quot; NILAI BENAR = 1&#13;&#10;  NILAI SALAH = 4&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </then>
                                                <else>
                                                    <assign variable="nilai" expression="0"/>
                                                    <output expression="&quot;Sebuah benda bergerak di atas lantai. Kecepatan benda tersebut semakin lama semakin kecil dan akhirnya berhenti. Gaya yang menyebabkan gerak benda tersebut semakin melambat adalah &#8230; .&#13;&#10;&#10;A. gaya dorong&#13;&#10;B. gaya gesekan&#13;&#10;C. gaya tarik&#13;&#10;D. gaya berat&quot;" newline="True"/>
                                                    <input variable="jawaban"/>
                                                    <if expression="jawaban = &quot;B&quot;">
                                                        <then>
                                                            <assign variable="nilai" expression="20"/>
                                                            <output expression="&quot; NILAI BENAR = 1&#13;&#10;  NILAI SALAH = 4&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <assign variable="nilai" expression="0"/>
                                                            <output expression="&quot; NILAI BENAR = 0&#13;&#10;  NILAI SALAH = 5&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="nilai" newline="True"/>
            <if expression="nilai &gt;= 60">
                <then>
                    <assign variable="hasil" expression="&quot;lulus&quot;"/>
                </then>
                <else>
                    <assign variable="hasil" expression="&quot;tidaklulus&quot;"/>
                </else>
            </if>
            <output expression="hasil" newline="True"/>
        </body>
    </function>
